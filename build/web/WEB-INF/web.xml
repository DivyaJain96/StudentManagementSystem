<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>notice</servlet-name>
        <servlet-class>notice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mail1</servlet-name>
        <servlet-class>mca.Mail1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatestudent</servlet-name>
        <servlet-class>UpdateStudentDetails.updatestudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStudents</servlet-name>
        <servlet-class>UpdateStudents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>downloadpdf</servlet-name>
        <servlet-class>PDF.downloadpdf</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadPDF1</servlet-name>
        <servlet-class>PDF.DownloadPDF1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download_student_attendance</servlet-name>
        <servlet-class>Excel.download_student_attendance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>U</servlet-name>
        <servlet-class>img.U</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>notice</servlet-name>
        <url-pattern>/notice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mail1</servlet-name>
        <url-pattern>/Mail1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatestudent</servlet-name>
        <url-pattern>/updatestudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStudents</servlet-name>
        <url-pattern>/UpdateStudents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>downloadpdf</servlet-name>
        <url-pattern>/downloadpdf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadPDF1</servlet-name>
        <url-pattern>/DownloadPDF1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download_student_attendance</servlet-name>
        <url-pattern>/download_student_attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>U</servlet-name>
        <url-pattern>/U</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
